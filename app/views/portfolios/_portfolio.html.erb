<p><%= link_to portfolio.title, portfolio_show_path(portfolio.id) %></p>

<p><%= portfolio.subtitle %></p>

<p><%= portfolio.body %></p>

<%= image_tag portfolio.thumb_image if !portfolio.thumb_image.nil?%>

<%= link_to "Edit Portfolio", edit_portfolio_path(portfolio.id)%>

<%= link_to 'Delete Portfolio', portfolio_path(portfolio.id), method: :delete, data: { confirm: 'Are you sure?' } %>